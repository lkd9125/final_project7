<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="calc" class="com.openai.di.Calculator"/>
	<bean id="mycal" class="com.openai.di.Mycal">
		<property name="cal">
			<ref bean = "calc" />
		</property>
		<property name="fnum" value="20" />
		<property name="fsum" value="2" />  <!-- 여기까지가 메인메서드에서의 객체생성 이후 변수설정까지 끝난거다 -->
	</bean>
	
	<bean id="tc" class="com.openai.di.TestClass">
		<property name="a" value="100" />
		<property name="b" value="100" />
	</bean>
	
</beans>
